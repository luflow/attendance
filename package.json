{
  "name": "attendance",
  "version": "1.17.0",
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  },
  "scripts": {
    "l10n:convert": "node scripts/l10n.js convert-po",
    "l10n:export": "node scripts/l10n.js export-po",
    "prebuild": "npm run l10n:convert",
    "build": "vite build",
    "dev": "vite --mode development build",
    "watch": "vite --mode development build --watch",
    "lint": "eslint src",
    "stylelint": "stylelint src/**/*.vue src/**/*.scss src/**/*.css",
    "test:e2e:server": "node tests/e2e/setup/server.js",
    "test:e2e:server-cleanup": "docker stop nextcloud-e2e-test-server_attendance 2>/dev/null || true && docker rm -f nextcloud-e2e-test-server_attendance 2>/dev/null || true && docker volume prune -f",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install --with-deps chromium"
  },
  "type": "module",
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "dependencies": {
    "@nextcloud/axios": "^2.5.1",
    "@nextcloud/dialogs": "^7.1.0",
    "@nextcloud/initial-state": "^2.2.0",
    "@nextcloud/l10n": "^3.4.0",
    "@nextcloud/router": "^3.0.1",
    "@nextcloud/vue": "^9.3.0",
    "canvas-confetti": "^1.9.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "marked": "^17.0.1",
    "vue": "^3.5.25",
    "vue-material-design-icons": "^5.3.1"
  },
  "devDependencies": {
    "@nextcloud/browserslist-config": "^3.0.1",
    "@nextcloud/e2e-test-server": "^0.4.0",
    "@nextcloud/eslint-config": "^8.4.2",
    "@nextcloud/stylelint-config": "^3.1.0",
    "@nextcloud/vite-config": "^2.5.2",
    "@playwright/test": "^1.57.0",
    "@vue/tsconfig": "^0.8.1",
    "sass-embedded": "^1.83.0",
    "typescript": "^5.7.2",
    "vite": "^7.2.4"
  }
}
